#: dialogs/textareaappearanceconfigwidget.ui:391
#, kde-format
msgid "I&ndent wrapped lines:"
msgstr "&Pievienot atkāpes aplauztajām rindām:"

#. i18n: ectx: property (text), widget (QLabel, lblDynamicWordWrapIndicators_2)
